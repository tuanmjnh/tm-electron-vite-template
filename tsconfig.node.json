{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/*",
    "src/preload/*",
    "src/services"
  ],
  "compilerOptions": {
    "composite": false,
    "types": [
      "electron-vite/node"
    ],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
