{
  "name": "tm-electron-vite-template",
  "version": "1.1.3",
  "private": false,
  "description": "An Electron application with Vue and TypeScript. GUI use Component Library Naive UI and Nova - Admin Template",
  "main": "./out/main/index.js",
  "author": {
    "name": "tuanmjnh",
    "email": "minhtuan200990@gmail.com",
    "url": "https://github.com/tuanmjnh"
  },
  "homepage": "https://github.com/tuanmjnh/tm-electron-vite-template",
  "repository": {
    "url": "https://github.com/tuanmjnh/tm-electron-vite-template.git"
  },
  "keywords": [
    "Electron",
    "ElectronVite",
    "Vue",
    "Vue3",
    "admin",
    "admin-template",
    "vue-admin",
    "vue-admin-template",
    "Vite5",
    "Vite",
    "vite-admin",
    "TypeScript",
    "TS",
    "NaiveUI",
    "naive-ui",
    "naive-admin",
    "NaiveUI-Admin",
    "naive-ui-admin",
    "UnoCSS"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@css-render/vue3-ssr": "^0.15.14",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@types/chrome": "^0.0.268",
    "@vueuse/core": "^11.0.1",
    "adm-zip": "^0.5.12",
    "alova": "^3.0.12",
    "axios": "^1.7.1",
    "better-sqlite3": "^10.0.0",
    "colord": "^2.9.3",
    "crypto-js": "^4.2.0",
    "echarts": "^5.5.1",
    "lodash.debounce": "^4.0.8",
    "md-editor-v3": "^4.18.1",
    "moment": "^2.30.1",
    "mongoose": "^8.4.0",
    "naive-ui": "^2.39.0",
    "node-downloader-helper": "^2.1.9",
    "node-machine-id": "^1.1.12",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "quill": "^2.0.2",
    "radash": "^12.1.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/icon-park-outline": "^1.1.16",
    "@iconify/vue": "^4.1.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/node": "^22.5.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.27.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "unocss": "^0.62.2",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-icons": "^0.19.2",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.4.2",
    "vue": "^3.4.38",
    "vue-tsc": "^2.0.29"
  }
}
